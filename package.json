{
  "name": "d3-blocks-template",
  "version": "1.0.2",
  "description": "setup for using es6 for bl.ocks.org",
  "main": "gist/script.js",
  "watch": {
    "build": {
      "patterns": [
        "script.js"
      ]
    }
  },
  "scripts": {
    "build": "buble gist/script.js > gist/script-compiled.js",
    "serve": "npm-run-all --parallel watch-js live-server",
    "watch-js": "npm-watch build",
    "live-server": "live-server gist --port=8888"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eesur/d3-blocks-template.git"
  },
  "author": "EESUR",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eesur/d3-blocks-template/issues"
  },
  "homepage": "https://github.com/eesur/d3-blocks-template#readme",
  "devDependencies": {
    "buble": "^0.15.1",
    "d3": "^4.9.1",
    "gistup": "^0.1.3",
    "live-server": "^1.1.0",
    "npm": "^4.0.5",
    "npm-run-all": "4.0.2",
    "npm-watch": "0.1.9"
  }
}
